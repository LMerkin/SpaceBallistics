FoamFile
{
  version       2.0;
  format        ascii;
  arch          "LSB;label=32;scalar=64";
  class         volScalarField;
  location      "0";
  object        alphat;
}
//===========================================================================//
// Thermal Eddy Diffusivity:
dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
  inLet
  {
    type    zeroGradient;
  }

  outLet
  {
    type    zeroGradient;
  }

  diamondWall
  {
    type    compressible::alphatWallFunction;
    value   uniform 0;
  }
}
