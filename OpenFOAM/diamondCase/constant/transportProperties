// vim:syntax=cpp:ts=2:et
/*--------------------------------*- C++ -*----------------------------------*\
| Transport properties file for compressible flow (air)                      |
\*---------------------------------------------------------------------------*/
foamFile
{
  version   2.0;
  format    ascii;
  class     dictionary;
  location  "constant";
  object    transportProperties;
}

#include "../VarParams"

thermophysicalModel  idealGas;  // The thermophysical model (ideal gas for air)
transportModel       Newtonian; // The viscosity model (for compressible flow)

idealGas
{
  R         287.0528; // Specific gas constant for air in J/(kg·K)
  Tref      298.15;   // Reference temperature (K)
  pRef      __p0__;   // Reference pressure (Pa)
  T0        273.15;   // Zero-degree temperature (K)
  L         1.4;      // Specific heat ratio (gamma) for air (ideal gas model)
}

viscosity
{
  mu        1.85e-5;  // Dynamic viscosity (Pa·s), @ 300K (XXX: close to Tref)
}


